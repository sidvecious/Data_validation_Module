{
    "test_df": {
        "columns": [
            {
                "name": "date_id",
                "type": "str",
                "validation": [
                    "check_date_format_YYYY_mm_dd"
                ]
            },
            {
                "name": "depth_id",
                "type": "str",
                "validation": [
                    "check_string_format_nnn_mmm"
                ]
            },
            {
                "name": "soil_texture",
                "type": "str",
                "validation": [
                    "check_type_of_row"
                ]
            },
            {
                "name": "db_id",
                "type": "int",
                "validation": [
                    "check_none_and_nan",
                    "check_positive_int"
                ]
            }
        ]
    },
    "single_column_test": {
        "columns": [
            {
                "name": "soc_percent",
                "type": "float",
                "validation": [
                    "check_range_from_zero_to_hundred"
                ]
            },
            {
                "name": "tolerance",
                "type": "int",
                "validation": [
                    "check_positive_int_or_Null"
                ]
            }
        ]
    }
}
